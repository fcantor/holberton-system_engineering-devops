# upstart file to run gunicorn

description "Gunicorn application server serving 6-number_odd_or_even"

start on runlevel [2345]
stop on runlevel [!2345]

respawn
setuid ubuntu
setgid www-data

env PATH=/home/ubuntu/AirBnB_clone_v2/env/bin
chdir /home/ubuntu/AirBnB_clone_v2
exec gunicorn --workers 3 --bind 0.0.0.0:8001 web_flask.6-number_odd_or_even:app
